# operadores_logicos
programa
{
	inclua biblioteca Matematica
 --> m
	
	funcao inicio()
	{
		escreva(2 * 4 == 24/3, "\n")
		escreva((15 % 4)< (19 % 6), "\n")
		escreva(nao (2 < 5) != (8 == 8), "\n")
		escreva(verdadeiro ou falso, "\n")
		escreva(2 * 2 == m.potencia (2,2), "\n")
		escreva(verdadeiro e ((7 / 2) > 3.5), "\n")
		escreva(m.arredondar(2.8, 0) == 3, " e ")
		escreva(m.raiz (9.0, 0) == 3)
		
	}
}
