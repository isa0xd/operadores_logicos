programa
{
	funcao inicio() 
	{ 
		real numero1, numero2, numero3, numero4 real maior

		escreva("Digite o primeiro valor:")
		leia (numero1)

		escreva("Digite o segundo valor:")
		leia (numero2)

		escreva("Digite o terceiro valor:")
		leia (numero3)

		escreva("Digite o quarto valor:")
		leia (numero4)

		se (numero1 > numero2) 
		se (numero1 > numero3) 
		se (numero1 > numero4) escreva (numero1)

		se (numero2 > numero1) 
		se (numero2 > numero3) 
		se (numero2 > numero4) escreva (numero2)

		se (numero3 > numero1) 
		se (numero3 > numero2) 
		se (numero3 > numero4) escreva (numero3)

		se (numero4 > numero1) 
		se (numero4 > numero2) 
		se (numero4 > numero3) escreva (numero4)
	} 
}
