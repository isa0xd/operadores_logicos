programa {
  funcao inicio() {
    real numero1, numero2
    real maior

    escreva("digite o primeiro valor: ")
    leia (numero1)

    escreva("digite o segundo valor: ")
    leia (numero2)

    se(numero1 > numero2) 
        escreva (numero1)

    se(numero2 > numero1) 
        escreva (numero2)
  }
}
